int Add(int x, int y) {
    return x + y;
}
int Sub(int x, int y) {
    return x - y;
}
int If(int x) {
    if (x)
        return 1;
    return 0;
}
int AddLocal(int x, int y) {
    int z = x + y;
    z += 1;
    return z;
}

int gadd;

int AddGlobal(int x, int y) {
    gadd += x + y;
    return gadd;
}

int AddLocalArr(int x, int y) {
    int z[3];
    z[0] = x;
    z[1] = y;
    z[2] = z[0] + z[1];
    return z[2];
}

int AddLocalArrFail(int x, int y) {
